{
  "presets": [
    "@babel/react",
    "@babel/stage-0"
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "createElement",
      "pragmaFrag": "Fragment"
    }],
    [
      "module-resolver",{
      "root" : ["./"],
      "cwd": "babelrc",
      "alias" : {
        "@ariesate/are": "../engine/src"
      }
    }
    ]
  ]
}
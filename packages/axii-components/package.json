  "dependencies": {
    "@ant-design/colors": "^5.0.1",
    "@ant-design/icons-svg": "^4.1.0",
    "@editorjs/editorjs": "^2.19.3",
    "@icon-park/svg": "^1.3.1",
    "axios": "^0.20.0",
    "codeflask": "^1.4.1",
    "history": "^5.0.0",
    "moment": "^2.28.0",
    "normalize.css": "^8.0.1",
    "path-to-regexp": "^6.1.0",
    "querystringify": "^2.2.0",
    "tui-grid": "^4.16.1",
    "tui-image-editor": "^3.14.0",
    "x-data-spreadsheet": "^1.1.8"
  },
  "peerDependencies": {
    "axii": "^1.0.3"
  },
  "type": "module",
  "devDependencies": {
    "command-line-args": "^5.1.1",
    "fast-json-patch": "^3.0.0-1",
    "less": "^3.12.2",
    "lodash": "^4.17.20",
    "vite": "^2.0.5"
  },
  "scripts": {
    "entry": "node ./scripts/generateIndexEntry.js",
    "start": "npm run entry && node ./scripts/copyPlaygroundAsString.js && vite --force --config vite.site.config.js",
    "build": "npm run entry && node ./scripts/copyPlaygroundAsString.js && vite build",
    "publish:patch": "npm run build && node ./scripts/publish.js --patch",
    "publish:minor": "npm run build && node ./scripts/publish.js --minor",
    "preview": "vite preview --config vite.site.config.js",
    "deploy": "./deploy.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/axii.umd.js",
  "module": "./dist/axii.es.js",
  "exports": {
    ".": {
      "import": "./dist/axii.es.js",
      "require": "./dist/axii.umd.js"
    }
  },

{
  "name": "axii",
  "version": "1.0.0",
  "description": "",
  "author": "sskyy <skyking_H@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@iusername/js-deep-clone": "^1.3.1",
    "fast-json-patch": "^3.0.0-1",
    "immer": "^7.0.8"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^2.0.0-next.9",
    "@prefresh/vite": "^2.2.0",
    "codemirror": "^5.60.0",
    "@testing-library/jest-dom": "^5.11.4",
    "jest": "^26.4.2",
    "jquery": "^3.5.1",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "pretty-data": "^0.40.0",
    "vite": "^2.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleDirectories": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "scripts": {
    "site": "vite --config vite.site.config.js",
    "build": "vite build",
    "build:site": "vite build --config vite.site.config.js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "files": ["dist"],
  "main": "./dist/axii.umd.js",
  "module": "./dist/axii.es.js",
  "exports": {
    ".": {
      "import": "./dist/axii.es.js",
      "require": "./dist/axii.umd.js"
    }
  }
}

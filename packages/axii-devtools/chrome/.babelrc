{
  "presets": [
    ["@babel/preset-env", {
      "exclude": [
        "@babel/plugin-transform-regenerator"
      ]
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "createElement",
      "pragmaFrag": "Fragment",
      "throwIfNamespace": false
    }],
    "babel-plugin-set-display-name",
    "@babel/plugin-proposal-class-properties"
  ]
}
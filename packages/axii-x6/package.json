{
  "name": "axii-x6",
  "version": "1.0.2",
  "author": "sskyy <skyking_H@hotmail.com>",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/axii-x6.umd.js",
  "module": "./dist/axii-x6.es.js",
  "exports": {
    ".": {
      "import": "./dist/axii-x6.es.js",
      "require": "./dist/axii-x6.umd.js"
    }
  },
  "scripts": {
    "start": "pnpm start --filter \"axii-x6-playground\"",
    "build": "vite build",
    "build:dev": "vite build --mode=development",
    "publish:patch": "node ./scripts/publish.js --patch",
    "publish:minor": "node ./scripts/publish.js --minor"
  },
  "dependencies": {
    "@antv/x6": "^1.20.0",
    "axii-components": "^1.0.32",
    "axii-icons": "^1.0.2",
    "axios": "^0.21.1",
    "copy-text-to-clipboard": "^2.2.0",
    "lodash-es": "^4.17.20"
  },
  "devDependencies": {
    "command-line-args": "^5.1.1",
    "less": "^3.12.2",
    "vite": "^2.1.5"
  },
  "peerDependencies": {
    "axii": "^1.0.8"
  }
}
